<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="522.0" prefWidth="1170.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.studentManagement.controller.StudentViewAllController">
   <children>
      <TabPane prefHeight="522.0" prefWidth="1170.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="viewAllStudentTab" onSelectionChanged="#viewAllStudentTabOnAction" text="Students">
            <content>
              <AnchorPane fx:id="viewAllStudentPane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
          <Tab fx:id="viewAllRegistrationTab" onSelectionChanged="#viewAllRegistrationTabOnAction" text="Registrations">
            <content>
              <AnchorPane fx:id="viewAllRegistrationPane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab fx:id="viewAllPaymentTab" onSelectionChanged="#viewAllPaymentTabOnAction" text="Payments">
              <content>
                <AnchorPane fx:id="viewAllPaymentPane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
